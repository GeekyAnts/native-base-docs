{
    "title": "NativeBase",
    "plugins": [
        "versions",
        "toggle-chapters",
        "codetabs",
        "include-codeblock",
        "ace",
        "copy-code-button",
        "edit-link"
    ],
    "pluginsConfig": {
        "edit-link": {
            "base": "https://github.com/GeekyAnts/native-base-docs/tree/master",
            "label": "Edit This Page"
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": "true",
            "theme": "monokai"
        },
        "versions": {
            "gitbookConfigURL": "http://rawgit.com/GeekyAnts/native-base-docs/master/book.json",
            "options": [
                {
                    "value": "http://docs.nativebase.io",
                    "text": "latest"
                },
                {
                    "value": "http://rawgit.com/GeekyAnts/native-base-docs/v0.5.20/_book/index.html",
                    "text": "v0.5.20"
                },
                {
                    "value": "http://docs.nativebase.io",
                    "text": "-- select version --",
                    "selected": false
                }
            ]
        }
    }
}